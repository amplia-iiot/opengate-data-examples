{
  "filter": {
    "eq": { "resourceType": "entity.device" }
  },
  "select": [
    {
      "name": "provision.device.identifier",
      "fields": [{ "field": "value", "alias": "ID" }]
    },
    {
      "name": "provision.device.administrativeState",
      "fields": [{ "field": "value", "alias": "STATE" }]
    },
    {
      "name": "provision.device.communicationModules[].mobile.imei",
      "fields": [{ "field": "value", "alias": "IMEI" }]
    }
  ],
  "limit": { "size": 25, "start": 1 }
}
